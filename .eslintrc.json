{
  "env": {
    "es2021": true,
    "node": true,
    "mocha": true,
    "mongo": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules":{
    "no-console": "off",
    "comma-dangle": "off",
    "no-underscore-dangle": "off",
    "quotes": ["error", "double"],
    "func-names": "off",
    "function-paren-newline": "off",
    "object-curly-newline": "off",
    "no-nested-ternary": "off",
    "import/prefer-default-export": "off"
  }
}
